datasource db {
    provider = "mysql"
    url = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
    output = "../src/generated/prisma"
}

model User {
    id String @id @default(uuid())
    name String
    email String @unique
    role Role @default(USER)
}

enum Role{
    USER
    ADMIN
}